<h1>Create a Song</h1>

<%= form_for @song  do |f| %>

  <%= f.label :title %> <br>
  <%= f.text_field :title %> <br>
  <%= f.label :artist_name %> <br>
  <%= f.text_field :artist_name %> <br>
  <%= f.label :genre_name %> <br>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %><br>
  <%= f.label :notes %><br>
  <input type= "text" name="song[song_notes][]" id="song_notes_1"><br><br>
  <input type= "text" name="song[song_notes][]" id="song_notes_2"><br><br>
  <input type= "text" name="song[song_notes][]" id="n3"><br><br>
  <input type= "text" name="song[song_notes][]" id="n4"><br><br>

  <br>
  <br>
  <%= f.submit %>

  <% end %>

